# Coverage configuration for backend
[run]
source = src/
omit = 
    src/**/__tests__/**
    src/**/*.test.js
    src/**/*.spec.js
    tests/**
    node_modules/**

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = coverage_html_report