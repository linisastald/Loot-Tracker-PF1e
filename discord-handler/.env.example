# Discord Configuration
DISCORD_PUBLIC_KEY=your_discord_public_key_here
PORT=3000

# Campaign Channel IDs (replace with your actual Discord channel IDs)
ROTR_CHANNEL_ID=your_rotr_channel_id
SNS_CHANNEL_ID=your_sns_channel_id  
TEST_CHANNEL_ID=your_test_channel_id

# Campaign API Endpoints (these should work with your Docker setup)
ROTR_API_ENDPOINT=http://rotr_app:5000/api
SNS_API_ENDPOINT=http://sns_app:5000/api
TEST_API_ENDPOINT=http://test_app:5000/api

# Optional: Override timeouts (milliseconds)
# REQUEST_TIMEOUT=2500
